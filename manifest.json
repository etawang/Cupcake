{
  "manifest_version": 2,

  "name": "Cupcake",
  "description": "Browser History",
  "version": "0.0.1",
  "options_page": "options.html",

  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  },

  "browser_action": {
    "default_icon": "icon.png"
  },

  "permissions": [
    "history",
    "tabs"
  ]
}
